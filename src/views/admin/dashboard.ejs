<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
            <th>ID</th>
            <th>BAŞLIK</th>
            <th>İÇERİK</th>
            <th>TÜR</th>
            <th>DURUM</th>
            <th>İŞLEMLER</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(function(request) { %>
                <tr>
                    <td><%= request.uuid %></td>
                    <td><%= request.title %></td>
                    <td><%= request.content %></td>
                    <td><%= request.requestTypeId %></td>
                    <td><%= request.status %></td>
                    <td><a href="/admin/request/<%= request.uuid %>" class="btn btn-sm btn-outline-secondary">Görüntüle</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>